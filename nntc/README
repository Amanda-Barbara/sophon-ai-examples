1.cmodel
  1.1 source ../scripts/envsetup_cmodel.sh #also can source envsetup_cmodel.sh bmnetc|bmnetm|bmnett|bmnetp|bmnetd
  1.2 cd bmnetc
    1.2.1 ./bmnetc_build_bmodel.sh
    1.2.2 bmrt_test --context_dir=output/det2/
    1.2.3 python3 bmnetc_build_bmodel.py
    1.2.4 bmrt_test --context_dir=python-output/det2/
  1.3 cd ../bmnetm
    1.3.1 ./bmnetm_build_bmodel.sh
    1.3.2 bmrt_test --context_dir=output/lenet/
    1.3.3 python3 bmnetm_build_bmodel.py
    1.3.4 bmrt_test --context_dir=python-output/lenet/
  1.4 cd ../bmnett
    1.4.1 ./bmnett_build_bmodel.sh
    1.4.2 bmrt_test --context_dir=output/vqvae/
    1.4.3 python3 bmnett_build_bmodel.py
    1.4.4 bmrt_test --context_dir=python-output/vqvae/
  1.5 cd ../bmnetp
    1.5.1 ./bmnetp_build_bmodel.sh
    1.5.2 bmrt_test --context_dir=output/anchors/
    1.5.3 python3 bmnetp_build_bmodel.py
    1.5.4 bmrt_test --context_dir=python-output/anchors/
  1.6 cd bmnetd
    1.6.1 ./bmnetd_build_bmodel.sh
    1.6.2 bmrt_test --context_dir=output/yolov3-tiny/
    1.6.3 python3 bmnetd_build_bmodel.py
    1.6.4 bmrt_test --context_dir=python-output/yolov3-tiny/
  1.7 cd bmtflite
    1.7.1 ./bmtflite_build_bmodel.sh
    1.7.2 bmrt_test --context_dir=compilation/


