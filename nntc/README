3.soc
  3.1 source ../scripts/envsetup_soc.sh
  3.2 copy det2 bmodel and other files generated in bmnetc pcie or cmodel mode to current directory
    3.2.1 bmrt_test --context_dir=output/det2/
    3.2.1 bmrt_test --context_dir=python-output/det2/
  3.3 copy lenet bmodel and other files generated in bmnetm pcie or cmodel mode to current directory
    3.3.1 bmrt_test --context_dir=output/lenet/
    3.3.2 bmrt_test --context_dir=python-output/lenet/
  3.4 copy vqvae bmodel and other files generated in bmnett pcie or cmodel mode to current directory
    3.4.1 bmrt_test --context_dir=output/vqvae/
    3.4.2 bmrt_test --context_dir=python-output/vqvae/
  3.5 copy anchors bmodel and other files generated in bmnetp pcie or cmodel mode to current directory
    3.5.1 bmrt_test --context_dir=output/anchors/
    3.5.2 bmrt_test --context_dir=python-output/anchors/
  3.6 copy yolov3-tiny bmodel and other files generated in bmnetd pcie or cmodel mode to current directory
    3.2.1 bmrt_test --context_dir=output/yolov3-tiny/
    3.2.1 bmrt_test --context_dir=python-output/yolov3-tiny/


4.runtime
  4.1 source ../scripts/envsetup_cmodel.sh bmnetc
  4.2 cd bmnetc && ./bmnetc_build_bmodel.sh
  4.2 cd ../runtime/classify/c_interface && cp Makefile.cmodel Makefile && make
  4.3 ./demo ../../../bmnetc/output/det2 ../test_fig/guys0.jpg
